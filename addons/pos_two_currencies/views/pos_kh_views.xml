<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="pos_config_form_view_inherit1" model="ir.ui.view">
            <field name="name">pos.config.form.view.inherit1</field>
            <field name="model">pos.config</field>
            <field name="inherit_id" ref="point_of_sale.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='pos_amount_authorized_diff']" position='after'>
                    <label string="Default Opening" for="proxy_ip" class="font-weight-normal" />
                    <field name="default_cashbox_id" />
                </xpath>
            </field>
        </record>

    </data>
</odoo>
